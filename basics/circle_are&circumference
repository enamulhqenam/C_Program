#include<stdio.h>

int main()
{
    float radius , area , Circumference ;
    float pi = 3.1416;
    
    printf("Enter radius of circle : ");
    scanf("%f", &radius);
    
    area = pi * radius * radius;
    printf("The circle are of = %.2f",area);
    
    Circumference = 2*pi*radius;
    printf("\nCircle Circumference is = %.2f",Circumference);
}
